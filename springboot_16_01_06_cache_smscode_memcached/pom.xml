    <groupId>top.Elsa</groupId>
    <artifactId>springboot_16_01_06_cache_smscode_memcached</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <properties>
        <java.version>1.8</java.version>
    </properties>
    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>

        <!--cache-->
        <dependency>
